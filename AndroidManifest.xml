<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hewaiming.slidebottomview.demo" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.hewaiming.slidebottomview.demo.SelectActivity"
            android:label="@string/title_activity_select" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.hewaiming.slidebottomview.demo.InputActivity"
            android:label="@string/title_activity_input" >
        </activity>
        <activity
            android:name="com.hewaiming.slidebottomview.demo.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.hewaiming.slidebottomview.demo.ScrollViewActivity"
            android:label="@string/title_activity_scroll_view" >
        </activity>
        <activity
            android:name="com.hewaiming.slidebottomview.demo.ListViewActivity"
            android:label="@string/title_activity_list_view" >
        </activity>
        <activity
            android:name="com.hewaiming.slidebottomview.demo.ViewPagerActivity"
            android:label="@string/title_activity_view_pager" >
        </activity>
    </application>

</manifest>